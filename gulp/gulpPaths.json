{
  "source" : "src",
  "build": "build",
  "dist": "dist",
  "test": "test",
  "resources": "resources",
  "vendor": "src/vendor",
  "assets": "src/assets",
  "data": "resources/data",
  "media": "resources/media",

  "html": "index.base.html",

  "less": "src/assets/styles/less",
  "excludeLess": [
    "!src/assets/styles/less/mixins.less",
    "!src/assets/styles/less/variables.less"
  ],
  "sass": "src/assets/styles/scss",
  "excludeSass": [
    "!src/assets/styles/scss/_mixins.scss",
    "!src/assets/styles/scss/_variables.scss"
  ],
  "css": "src/assets/styles/css",
  "cssFile": "portfolio.css",

  "js": "src/assets/scripts/js",
  "excludeJs": "!src/assets/scripts/js/**/*.min.js",
  "jsOut" : "src/assets/scripts/js/out",

  "angularApp": "src/angular",

  "allScripts": [
    "gulpfile.js",
    "!src/**/*.min.js",
    "src/**/*.js",
    "!src/assets/js/*.js",
    "src/assets/js/*.js",
    "test/**/*.js"
  ],

  "vendors": [
    "src/vendor/angular-animate",
    "src/vendor/angular-touch",
    "src/vendor/angularjs",
    "src/vendor/angular-ui-bootstrap",
    "src/vendor/bootstrap",
    "src/vendor/font-awesome",
    "src/vendor/jquery"
  ]

}